    "jasmine": "^3.1.0",
    "jsdoc-api": "^4.0.3",
    "jsdom": "^11.9.0",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.5",
    "lodash-es": "^4.17.10",
    "markdown-it": "^8.4.1",
    "marked": "^0.3.19",
    "megamatch": "dasdeck/megamatch",
